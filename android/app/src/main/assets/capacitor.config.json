{
	"appId": "com.dathuynh.lovealarm",
	"appName": "Love Alarm",
	"webDir": "www",
	"bundledWebRuntime": false,
	"plugins": {
		"PushNotifications": {
			"presentationOptions": [
				"badge",
				"sound",
				"alert"
			]
		},
		"SplashScreen": {
			"launchAutoHide": true,
			"launchShowDuration": 0,
			"splashImmersive": true
		}
	}
}
